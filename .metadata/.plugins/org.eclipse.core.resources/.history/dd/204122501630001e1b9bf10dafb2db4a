package day15;

import java.util.Stack;

public class Assignment_day15_3 {
	
	public static int baseballGame(String [] s) {
		int sum=0;
		Stack<Integer> stack = new Stack<>();
		
		for(int i=0;i<s.length;i++) {
			String s=[i];
			
			
			if(Integer.isDgit(ch)) {
				stack.push(Integer.parseInt(""+ch));
				sum+=Integer.parseInt(""+ch);
				System.out.println("push"+Integer.parseInt(""+ch));
			}
			
			if(ch.equals( '+')) {
				int val1 = stack.pop();
				int val2 = stack.pop();
				stack.push(val2); //push back the 2 vals into the stack after adding them
				stack.push(val1);
				
				sum+=val1+val2;
				
			}
			
			if(ch=='D') {
				sum-=(2*stack.peek());
			}
			
			if(ch == 'C') {
				int val = stack.pop();
				sum-=val;
			}
			
			
		}
		
		return sum;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String[] s = new String[]{"5","-2","4","C","D","9","+","+"};
	
		System.out.println(baseballGame(s));

	}

}
